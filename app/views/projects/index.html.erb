<% @projects.each do |project| %>
  <% if current_user.admin? %>
    <ul>
      <li> <%= link_to project.title, project %> </li>
    </ul>
    <%= link_to "delete", project, method: :delete, data: { confirm: "You sure?" } %>
  <% elsif current_user.id == project.user_id %>
    <ul>
      <li> <%= link_to project.title, project %> </li>
    </ul>
  <% end %>
<% end %>


<%= week_calendar events: @projects do |date, projects| %>
  <%= date %>

  <% projects.each do |project| %>
    <div>
      <%= project.title %>
    </div>
  <% end %>
<% end %>