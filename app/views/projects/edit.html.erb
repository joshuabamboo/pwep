<%= form_for @project do |f| %>
  <%= f.label :title, "Project Title" %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>
  <br>
  <%= f.label :starts_at, "Estimated Completion" %>
  <%= f.date_field :starts_at %>
  <br>
  <%= f.submit %>
<% end %>